@model sGridServer.Code.DataAccessLayer.Models.MultiLanguageString
@{
    sGridServer.Code.DataAccessLayer.Models.MultiLanguageStringExtensionMethods.InsertMissingTranslations(Model);
}
<div class="sg_language_string_container">
@Html.HiddenFor(model => model.Id)
@for (int i = 0; i < Model.Translations.Count; i++)
{
    <img src="@Url.Content(sGridServer.Code.Utilities.LanguageManager.LanguageByCode(Model.Translations[i].Culture).IconUrl)" />
    @Html.EditorFor(model => model.Translations[i].Text)
    @Html.HiddenFor(model => model.Translations[i].Culture)
    @Html.HiddenFor(model => model.Translations[i].Id)
} 
</div>